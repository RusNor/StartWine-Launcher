##################################   STOP   ####################################

STOP () {

    wine_pids="$(ls -l /proc/*/exe 2>/dev/null | grep -E 'wine(64)?-preloader|wineserver' | awk -F/ '{print $3}')"
    if [ ! -z "${wine_pids}" ]; then
        kill -n 9 ${wine_pids}
    fi

    notify-send -a "StartWine" "$(eval_gettext "Kill wine porocesses")"

    exit

}

