msgstr "基于 OpenGL 或 vulkan 的渲染引擎版本"

#: sw_data.py:1988
msgid "Shows current Wine or Proton version in use"
msgstr "显示当前使用的 Wine 或 Proton 版本"

#: sw_data.py:1989
msgid "Frametime next to fps text"
msgstr "fps 文本旁边的帧时间"

#: sw_data.py:1990
